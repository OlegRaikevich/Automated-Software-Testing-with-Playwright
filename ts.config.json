{
    "compilerOptions": {
      "target": "ESNext",
      "module": "commonjs",
      "lib": ["ESNext", "DOM"],
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "moduleResolution": "node",
      "types": ["node"]
    },
    "include": [
      "page-objects/**/*.ts",
      "tests/**/*.ts"
    ]
  }
  